<%= form_for(@reviewer) do |f| %>
	<div class="field">
		<p>name</p>
		<%= f.text_field :name%>
	</div>
	<div class="field">
		<p>location</p>
		<%= f.text_field :location%>
	</div>
	<div class="field">
		<p>email</p>
		<%= f.text_field :email%>
	</div>
	<div class="actions">
		<%= f.submit %>
	</div>
<% end %>



